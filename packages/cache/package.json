{
  "name": "@anchan828/nest-cache",
  "version": "3.1.14",
  "description": "",
  "homepage": "https://github.com/anchan828/nest-cache#readme",
  "bugs": {
    "url": "https://github.com/anchan828/nest-cache/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchan828/nest-cache.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "anchan828 <anchan828@gmail.com>",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "copy:license": "cp ../../LICENSE ./",
    "copy:readme": "cp ../../README.md ./",
    "lint": "TIMING=1 eslint '**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run copy:license && npm run copy:readme",
    "test": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk ../../node_modules/jest/bin/jest --runInBand --logHeapUsage",
    "test:watch": "npm run test -- --watch",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@anchan828/nest-cache-common": "^3.1.14",
    "cache-manager": "^5.7.6"
  },
  "devDependencies": {
    "@anchan828/nest-cache-manager-async-local-storage": "^3.1.14",
    "@anchan828/nest-cache-manager-ioredis": "^3.1.14",
    "@anchan828/nest-cache-manager-memory": "^3.1.14",
    "@nestjs/cache-manager": "2.3.0",
    "@nestjs/common": "10.4.5",
    "@nestjs/config": "3.3.0",
    "rxjs": "7.8.1"
  },
  "peerDependencies": {
    "@nestjs/cache-manager": "^2.2.2"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "npm@10.9.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.js"
    }
  }
}