{
  "name": "@anchan828/nest-cache",
  "version": "3.1.9",
  "description": "",
  "homepage": "https://github.com/anchan828/nest-cache#readme",
  "bugs": {
    "url": "https://github.com/anchan828/nest-cache/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchan828/nest-cache.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "anchan828 <anchan828@gmail.com>",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "copy:license": "cp ../../LICENSE ./",
    "copy:readme": "cp ../../README.md ./",
    "lint": "TIMING=1 eslint '**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run copy:license && npm run copy:readme",
    "test": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk ../../node_modules/jest/bin/jest --runInBand --logHeapUsage",
    "test:watch": "npm run test -- --watch",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@anchan828/nest-cache-common": "^3.1.9",
    "cache-manager": "^5.2.3"
  },
  "devDependencies": {
    "@anchan828/nest-cache-manager-async-local-storage": "^3.1.9",
    "@anchan828/nest-cache-manager-ioredis": "^3.1.9",
    "@anchan828/nest-cache-manager-memory": "^3.1.9",
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/common": "10.4.2",
    "@nestjs/config": "3.2.3",
    "rxjs": "7.8.1"
  },
  "peerDependencies": {
    "@nestjs/cache-manager": "^2.1.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "npm@10.8.3",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.js"
    }
  }
}